    <dbpprop:town rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Saint-Nazaire</dbpprop:town>
    <sc:openingHours rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mi-décembre</sc:openingHours>
    <dbpedia-owl:department rdf:datatype="http://www.w3.org/2001/XMLSchema#string">44</dbpedia-owl:department>
