    <foaf:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LE PRINTEMPS DU LIVRE DE MONTAIGU</foaf:name>
  </rdf:Description>
  <rdf:Description rdf:about="http://lodpaddle.com/RDV_litteraires/LA_25E_HEURE_DU_LIVRE">
