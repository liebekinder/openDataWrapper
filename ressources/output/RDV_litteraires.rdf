  <rdf:Description rdf:about="http://projet.com/8/LE_PRINTEMPS_DU_LIVRE_DE_MONTAIGU">
    <ex:organisateur rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Communauté de communes Terres de Montaigu</ex:organisateur>
    <ex:siteWeb rdf:datatype="http://www.w3.org/2001/XMLSchema#string">www.terresdemontaigu.fr</ex:siteWeb>
    <ex:codeInsee rdf:datatype="http://www.w3.org/2001/XMLSchema#string">85146</ex:codeInsee>
    <ex:departement rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">85</ex:departement>
    <ex:ville rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Montaigu</ex:ville>
