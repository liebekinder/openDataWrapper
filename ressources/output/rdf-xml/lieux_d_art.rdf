  <rdf:Description rdf:about="http://lodpaddle.com/lieux_d_art/ABBAYE_DE_FONTEVRAUD_">
    <foaf:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ABBAYE DE FONTEVRAUD </foaf:name>
    <dbpprop:town rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fontevraud-l’Abbaye</dbpprop:town>
    <sc:price rdf:datatype="http://www.w3.org/2001/XMLSchema#string">undefined</sc:price>
    <dbpedia-owl:postalCode rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">49590</dbpedia-owl:postalCode>
