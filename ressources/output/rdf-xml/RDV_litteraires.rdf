  </rdf:Description>
  <rdf:Description rdf:about="http://lodpaddle.com/RDV_litteraires/ESCALE_2012_DU_SALON_DU_LIVRE_DE_MER_DE_L’ÎLE_DE_NOIRMOUTIER">
