    <dbpprop:town rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mûrs-Érigné</dbpprop:town>
    <sc:openingHours rdf:datatype="http://www.w3.org/2001/XMLSchema#string">31 mars-1er avril</sc:openingHours>
    <dbpedia-owl:department rdf:datatype="http://www.w3.org/2001/XMLSchema#string">49</dbpedia-owl:department>
    <sc:telephone rdf:datatype="http://www.w3.org/2001/XMLSchema#string">02 41 57 81 85</sc:telephone>
    <dbpedia-owl:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#string">14e édition qui réunit nombre de librairies anciennes et d’occasion ainsi que quelques artisans (relieur, enlumineur) autour d’expositions et d’animations. Les vieux métiers seront à l’honneur pour 2012.</dbpedia-owl:abstract>
    <sc:performer rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Centre culturel Jean-Carmet</sc:performer>
    <dbpprop:insee rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">49223</dbpprop:insee>
    <foaf:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SALON ANGEVIN DU LIVRE ANCIEN</foaf:name>
  </rdf:Description>
